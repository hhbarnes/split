{
	"extends": "airbnb-base",
	"rules": {
		"indent": [ "error", "tab", { "SwitchCase": 1, "MemberExpression": 0  } ],
		"no-tabs": 0,
		"semi": ["error", "always"],
		"linebreak-style": ["error", "windows"],
		"no-underscore-dangle": ["error", { "allow": ["_id"] }],
		"newline-per-chained-call": ["error", { "ignoreChainWithDepth": 2 }],
		"no-console":"off",
	}
}